                ".amp-iconfont": {
                  "color": "@primaryColor",
                  "vertical-align": "bottom",
                  "font-size": "~\"@{iconsSize}rem\"",
                  "width": "~\"@{iconsSize}rem\"",
                  "@media (max-width: 767px)": {
